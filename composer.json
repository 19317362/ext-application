{
    "name": "teqneers/ext-application",
    "version": "0.1.x-dev",
    "type": "library",
    "description": "A base component to integrate Sencha Ext JS into a PHP application",
    "homepage": "https://github.com/teqneers/ext-application",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Gehrig",
            "email": "gehrig@teqneers.de",
            "homepage": "http://opensource.teqneers.de/"
        }
    ],
    "autoload": {
        "psr-4": {
            "TQ\\ExtJS\\Application\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TQ\\ExtJS\\Application\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "phpunit/php-token-stream": "1.4.6"
    }
}
